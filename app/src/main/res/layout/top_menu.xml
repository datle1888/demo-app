<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

  <data>

    <import type="android.view.View" />

    <import type="com.jst.poc.dispatcher.models.Top_selection" />

    <variable
      name="dat"
      type="com.jst.poc.dispatcher.models.BeanSelectedPage" />
  </data>

  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:orientation="horizontal">

    <Button
      android:id="@+id/dashBtn1"
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_margin="5dp"
      android:layout_weight="1"
      android:text="Status" />

    <Button
      android:id="@+id/dashBtn5"
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_margin="5dp"
      android:layout_weight="1"
      android:text="Jobs" />

    <Button
      android:id="@+id/dashBtn2"
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_margin="5dp"
      android:layout_weight="1"
      android:text="Dashboard" />

    <Button
      android:id="@+id/dashBtn3"
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_margin="5dp"
      android:layout_weight="1"
      android:text="Vehicle" />

    <Button
      android:id="@+id/dashBtn4"
      android:layout_width="wrap_content"
      android:layout_height="match_parent"
      android:layout_margin="5dp"
      android:layout_weight="1"
      android:text="Alerts" />
  </LinearLayout>
</layout>